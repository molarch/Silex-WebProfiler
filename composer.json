{
    "name": "molarch/web-profiler",
    "description": "A WebProfiler for Silex",
    "homepage": "http://silex.sensiolabs.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "John Layt",
            "email": "jlayt@mola.org.uk"
        }
    ],
    "require": {
        "molarch/silex": "dev-master",
        "symfony/web-profiler-bundle": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/twig-bridge": "^5.4",
        "symfony/stopwatch": "^5.4"
    },
    "conflict": {
        "symfony/web-profiler-bundle": "3.1.0"
    },
    "replace": {
        "silex/web-profiler": "self.version"
    },
    "require-dev": {
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/translation": "^5.4",
        "rector/rector": "^0.18.0"
    },
    "autoload": {
        "psr-4": { "Silex\\Provider\\": "" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/molarch/silex"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
